const SoninYm = () => {
      inputNeg.innerHTML = Number(Math.floor(Math.random() * 100));
      inputHoyr.innerHTML = Number(Math.floor(Math.random() * 100));
  if (Number(inputNeg.innerHTML) + Number(inputHoyr.innerHTML) === Number(inputGurav)) {
    alert("it/'s right");
  } else {
    alert("Wrong");
  }

};
buttonNeg.addEventListener("click", SoninYm);

